{
  "name": "discord-quiz-bot",
  "version": "1.0.0",
  "description": "A Discord bot for hosting interactive quizzes with leaderboards",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "prepare": "husky install",
    "deploy-commands": "tsx scripts/deploy-commands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "quiz",
    "typescript",
    "discord.js"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@discordjs/ws": "^1.0.2",
    "@prisma/client": "^5.7.1",
    "@types/uuid": "^10.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
